{
  "multipart": [
    {
      "when":  {"facing":  "north", "has_oil":  false},
      "apply": { "model": "extfar:block/frier", "y": 180  }
    },
    {
      "when":  {"facing":  "north", "has_oil":  true},
      "apply": { "model": "extfar:block/frier_full", "y": 180  }
    },
    {
      "when":  {"facing":  "south", "has_oil":  false},
      "apply": { "model": "extfar:block/frier", "y": 0  }
    },
    {
      "when":  {"facing":  "south", "has_oil":  true},
      "apply": { "model": "extfar:block/frier_full", "y": 0  }
    },
    {
      "when":  {"facing":  "east", "has_oil":  false},
      "apply": { "model": "extfar:block/frier", "y": 90  }
    },
    {
      "when":  {"facing":  "east", "has_oil":  true},
      "apply": { "model": "extfar:block/frier_full", "y": 90  }
    },
    {
      "when":  {"facing":  "west", "has_oil":  false},
      "apply": { "model": "extfar:block/frier", "y": 270  }
    },
    {
      "when":  {"facing":  "west", "has_oil":  false},
      "apply": { "model": "extfar:block/frier_full", "y": 270  }
    }
  ]
}
